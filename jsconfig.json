{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@src/*": ["src/*"],
            "@components/*": ["src/component/*"],
            "@images/*": ["src/assets/images/*"],
            "@global/*": ["src/assets/style/*"],
            "@public/*": ["public/*"],
            "@constants/*": ["src/constants/*"]
        }
    },
    "exclude": ["node_modules", "build"]
}
